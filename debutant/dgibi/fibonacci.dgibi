OPTI 'SORT' 'fibonacci.txt'  ;

SORT 'CHAI' 'Hello' ;
SORT 'CHAI' 4 8 15 16 23 42 ;
SORT 'CHAI' ' ' ;

SORT 'CHAI' 'Iter.  Fibo' ;
fibo1 = 0 ;
fibo2 = 1 ;
SORT 'CHAI' 0  fibo1 ;
SORT 'CHAI' 1  fibo2 ;
REPE b1 15 ;
  fibo3 = fibo2 ;
  fibo2 = fibo1 + fibo2 ;
  fibo1 = fibo3 ;
  SORT 'CHAI' (&b1 + 1) fibo2 ;
FIN  b1 ;

FIN ;